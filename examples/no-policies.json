      "enabled": false,
      "isBlocking": false,
      "minimumApproverCount": 1,
      "creatorVoteCounts": false,
      "allowDownvotes": true,
      "resetOnSourcePush": false
    },
    "workItemLinking": {
      "enabled": false,
      "isBlocking": false
    },
    "commentResolution": {
      "enabled": false,
      "isBlocking": false
    },
    "buildValidation": {
      "enabled": false,
      "isBlocking": false,
      "buildDefinitionId": 0,
      "displayName": "",
      "validDuration": 0,
      "queueOnSourceUpdateOnly": false
    },
    "statusCheck": {
      "enabled": false,
      "isBlocking": false,
      "statusName": "",
      "statusGenre": "",
